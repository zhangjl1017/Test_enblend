$(srcdir)/stamp-vti: enblend.texi $(top_srcdir)/configure
	@(dir=.; test -f ./enblend.texi || dir=$(srcdir); \
	set `$(SHELL) $(top_srcdir)/mdate-sh $$dir/enblend.texi`; \
	echo "@set UPDATED $$1 $$2 $$3"; \
	echo "@set UPDATED-MONTH $$2 $$3"; \
	echo "@set EDITION $(VERSION)"; \
	echo "@set VERSION $(VERSION)") > vti.tmp
	@cmp -s vti.tmp $(srcdir)/versenblend.texi \
	  || (echo "Updating $(srcdir)/versenblend.texi"; \
	      cp vti.tmp $(srcdir)/versenblend.texi)
	-@rm -f vti.tmp
	@cp $(srcdir)/versenblend.texi $@

mostlyclean-vti:
