	for x in $(HTMLS); do \
          xhtml=`basename $$x .html`.xhtml; \
	  if test -d $$xhtml; then \
	    for y in $$xhtml/*.xhtml; do \
	      $(XMLLINT) $(XMLLINT_FLAGS) $$y; \
	    done; \
	  else \
	    $(XMLLINT) $(XMLLINT_FLAGS) $$xhtml; \
          fi; \
	done

